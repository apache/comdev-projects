  <asfext:pmc rdf:about="karaf">
    <asfext:name>Apache Karaf</asfext:name>
    <foaf:homepage rdf:resource="http://karaf.apache.org/"/>
    <asfext:chair>
      <foaf:Person>
        <foaf:name>Guillaume Nodet</foaf:name>
      </foaf:Person>
