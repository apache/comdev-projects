<rdf:RDF xml:lang="en"
         xmlns="http://usefulinc.com/ns/doap#" 
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:asfext="http://projects.apache.org/ns/asfext#"
         xmlns:foaf="http://xmlns.com/foaf/0.1/">
  <asfext:pmc rdf:about="hawq">
    <asfext:name>Apache HAWQ</asfext:name>
    <asfext:charter>
    The mission of Apache HAWQ is the creation and maintenance of software
    related to a Hadoop native SQL query engine that
       combines the key technological advantages of an MPP database
       with the scalability and convenience of Hadoop
    </asfext:charter>
    <foaf:homepage rdf:resource="http://hawq.apache.org/"/>
  </asfext:pmc>
</rdf:RDF>
